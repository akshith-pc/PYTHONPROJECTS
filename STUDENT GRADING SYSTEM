total=0
student=input("enter the student name:")
for i in range(5):
    marks = float(input("Enter marks for subject :"))
    total+=marks
average=total/5
if average>90:
    result="A"
elif average>80:
    result="B"
elif average>70:
    result="C"
print("THE STUDENT NAME IS",student,'\n',"THE TOTAL IS:",total, '\n',"THE AVERAGE IS",average,'\n',
#    "THE GRADE IS",result)   

